{
    "version": "0.2.0",
    "configurations": [
        {
            "runtimeExecutable": "/usr/bin/chromium",
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/hardware-tycoon-api/bin/Debug/net5.0/hardware-tycoon-api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/hardware-tycoon-api",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "runtimeExecutable": "/usr/bin/chromium",
        },
        {
            "type": "pwa-chrome",
            "runtimeExecutable": "/usr/bin/chromium",
            "name": "http://127.0.0.1:3000/frontend/index.html",
            "request": "launch",
            "url": "http://127.0.0.1:3000/frontend/index.html"
        }
    ]
}